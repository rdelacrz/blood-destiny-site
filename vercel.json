{
  "rewrites": [
    {
      "source": "/((?!assets/).*)",
      "has": [
        {
          "type": "host",
          "value": "vercel.app"
        }
      ],
      "destination": "/api/ssr.js"
    },
    {
      "source": "/_telefunc",
      "destination": "/api/telefunc.js"
    }
  ],
  "framework": "vite",
  "outputDirectory": "dist/client"
}